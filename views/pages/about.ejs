<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head', {title}) %>
</head>
<body>
    <%- include('../partials/header') %>
    
    <main class="container">
        <h1>About Our Team</h1>
        
        <div class="team-grid">
            <% team.forEach(member => { %>
                <div class="team-card">
                    <h3><%= member.name %></h3>
                    <p><%= member.role %></p>
                </div>
            <% }); %>
        </div>
    </main>

    <%- include('../partials/footer') %>
</body>
</html>
